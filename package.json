{
   "name": "eli_api",
   "version": "0.0.1",
   "engines": {
      "node": ">=8.0.0"
   },
   "description": "ELI Project Backend",
   "devDependencies": {
      "@types/express": "^4.17.9",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^4.13.0",
      "@typescript-eslint/parser": "^4.13.0",
      "eslint": "^7.18.0",
      "eslint-config-prettier": "^7.1.0",
      "eslint-plugin-prettier": "^3.3.1",
      "nodemon": "^2.0.6",
      "prettier": "^2.2.1",
      "ts-node": "^9.1.1",
      "ts-node-dev": "^1.1.1",
      "tsc-watch": "^4.2.9",
      "typescript": "^3.9.7"
   },
   "dependencies": {
      "apollo-server": "^2.19.0",
      "apollo-server-express": "^2.19.0",
      "bcrypt": "^5.0.0",
      "class-validator": "^0.12.2",
      "cookie-parser": "^1.4.5",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "graphql": "^15.4.0",
      "jsonwebtoken": "^8.5.1",
      "mysql": "^2.14.1",
      "reflect-metadata": "^0.1.10",
      "type-graphql": "^1.1.1",
      "typeorm": "0.2.29"
   },
   "scripts": {
      "start": "npm run dev",
      "build": "tsc",
      "lint": "eslint . ",
      "lint-fix": "eslint --fix . ",
      "dev": "ts-node-dev --transpile-only --respawn  ./src/index.ts",
      "prod": "tsc && node ./built/src/server.js",
      "debug": "tsc && node --inspect ./built/src/server.js",
      "prettier": "prettier --write ./src"
   },
   "node/no-unsupported-features/es-syntax": [
      "error",
      {
         "version": ">=8.0.0",
         "ignores": []
      }
   ]
